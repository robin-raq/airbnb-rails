{
    "files.associations": {
    "*.html.erb": "html",
    "*.js.erb": "javascript",
    "*.rb": "ruby",
    "*.rake": "ruby",
    "Gemfile": "ruby",
    "Gemfile.lock": "ruby"
  },
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "rubyLsp.formatter": "syntax_tree",
  "editor.formatOnSave": true,
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp"
  }

}
